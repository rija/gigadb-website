[s3_metadata]
type = s3
provider = AWS
access_key_id = {{ credentials.list.1["Access Key Id"] }}
secret_access_key = {{ credentials.list.1["Secret Access Key"] }}
region = ap-northeast-1
location_constraint = ap-northeast-1
acl = private

[wasabi_{{ credentials.list.0["User Name"] }}]
type = s3
env_auth = true
region = ap-northeast-1
endpoint = s3.ap-northeast-1.wasabisys.com
location_constraint =
acl = public-read
server_side_encryption =
storage_class =
provider = Other
no_check_bucket = true
